<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.jamesZhan.entity.SensitiveportEntity" table="sensitiveport" schema="firewalllog">
        <id name="id">
            <column name="id" sql-type="int(3)"/>
        </id>
        <property name="port">
            <column name="port" sql-type="int(6)"/>
        </property>
        <property name="desc">
            <column name="desc" sql-type="varchar(200)" length="200" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>